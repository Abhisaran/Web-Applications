{
  
"content_scripts": [
    {
      "all_frames": true,
      "js": [
        "defaults.js",
        "content.js"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  
"description": "Prevents the blocking of copying from & pasting into input fields",

"homepage_url": "http://www.abhisaran.tk",

"manifest_version": 2,

"name": "I Defy You",


"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
"options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
"permissions": [
    "storage"
  ],
  "version": "1.0"
}